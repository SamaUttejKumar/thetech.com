<%= form_tag create_specific_submodule_homepages_path, remote: true do %>
  <%= hidden_field_tag :type, 'links' %>
  <%= hidden_field_tag :uuid, uuid %>
  <%= hidden_field_tag :mod_uuid, mod_uuid %>

  <%= render 'modules/forms/article_select', name: 'links', multiple: true %>

  <%= submit_tag 'Add Links' %>
<% end %>
