- likely_authors = (Author.recent_photographers.to_a + Author.new_authors.to_a).sort_by(&:name)
- articles = Article.order("created_at DESC").limit(50)
- images = local_assigns[:images] || []
- fetch = local_assigns[:fetch] || nil

= react_component 'ImageList', {images: images.map { |i| i.as_react(current_ability) }, authors: likely_authors.map { |a| a.as_react(current_ability) }, articles: articles.map { |a| a.as_react(current_ability) }, fetch: fetch }
