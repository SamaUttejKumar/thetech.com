<% if @page == 1 %>
  $('.images-table tbody').html("<%=j render 'image_list_tbody', images: @images %>")
<% else %>
  $("<%=j render 'image_list_tbody', images: @images %>").insertBefore('[data-autoscroll-target]');

  <% if @autoscroll_target.present? %>
    $('[data-autoscroll-target]').data('autoscroll-target', "<%= @autoscroll_target %>");
  <% else %>
    $('[data-autoscroll-target]').removeData('autoscroll-target');
  <% end %>
<% end %>