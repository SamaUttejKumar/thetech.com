<%= render 'frontend_homepage/nameplate' %>

<img src="" alt="" class="ad-homepage-top" data-ads="homepage_top">

<% if @homepage_editing || @homepage_locked %>
  <%= render 'master_editing_toolbar', homepage: @homepage %>
<% end %>

<% @homepage.layout.each do |row| %>
